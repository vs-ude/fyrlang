type S struct {
    X int
    Y int
    Next *S
    Iso -> *S
}

func test1(pass int) {
	var grp2 uintptr = 0
	for i := 0; i < pass; i++ {
		var ptr2 mut *S = {}
		b := groupOf(ptr2)
		if b == grp2 {
			panic("Should be differen groups in each iteration")
		}
		grp2 = b
		if i == 1 {
			break
		}
	}
}

func Main() {
	println("test1(0) ...")
	test1(0)
	println("test1(2) ...")
	test1(2)
	println("Ok")
}
